<?xml version="1.0" encoding="Cp1251" ?>
<query>
	<select><![CDATA[
SELECT dc.DCMCRD_ID, dc.NAME, dc.ORDER_NUMBER, dc.CHANGE_DATE, dcd.BINARY_DATA 
FROM DOC_BASE.DOCUMENT_CARDS dc 
LEFT JOIN DOC_BASE.DOCUMENT_CONTENTS dc2  
ON dc.DCMCRD_ID = dc2.DCMCRD_DCMCRD_ID 
LEFT JOIN DOC_BASE.DOCUMENT_CONTENT_DATA dcd 
ON dc2.DCMCNTNT_ID = dcd.DCMCNTNT_DCMCNTNT_ID 
WHERE dc.NAME like '%jpg' or dc.NAME like '%png'  
ORDER BY dc.NAME FETCH NEXT 100 ROWS ONLY
	]]></select>
	<order-by></order-by>
</query>